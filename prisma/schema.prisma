datasource db {
  provider = "postgresql"
  url      = "postgres://postgres:12345678@postgres:5432/testeTecnico"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

model User {
  id         Int      @id @default(autoincrement())
  name       String
  wage       Int
  enterprise Int
  isSelected Boolean
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
