generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  name       String
  wage       Int
  enterprise Int
  isSelected Boolean
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
